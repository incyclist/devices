{
	"compilerOptions": {
    "target": "esnext",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "sourceMap": true,
    "baseUrl": "./src",
    "outDir": "./build",
	"module": "esnext",
    "declaration": false,
    "removeComments": true
    },
    "include": [
        "./src/**/*.ts"
      ],
    "exclude": [
        "src/**/*.test.*",
      ]
}